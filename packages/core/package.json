{
"name": "marrakech",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./analyze": {
        "types": "./dist/analyze.d.ts",
        "import": "./dist/analyze.js",
        "require": "./dist/analyze.cjs"
    },
    "./optimize": {
        "types": "./dist/optimize.d.ts",
        "import": "./dist/optimize.js",
        "require": "./dist/optimize.cjs"
    }
  },
  "files": ["dist", "README.md"],
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "biome lint src",
    "format": "biome format --write src"
  },
  "devDependencies": {
    "zod": "^3.23.0",
    "tsup": "^8.0.0",
    "vitest": "^1.0.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  }
}
