{
"name": "@marrakesh/cli",
"version": "0.1.0-alpha.2",
"description": "CLI for testing Marrakesh prompts",
"license": "Apache-2.0",
  "type": "module",
  "bin": {
    "marrakesh": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@marrakesh/core": "workspace:*",
    "commander": "^12.0.0",
    "glob": "^10.3.10",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "chokidar": "^3.5.3",
    "tsx": "^4.0.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "vitest": "^1.0.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  }
}

